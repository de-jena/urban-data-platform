{
	":configurator:resource-version": 1,
	":configurator:version": "1.0.0",
	":configurator:symbolic-name": "Civitas Http",
		"org.apache.felix.http~mdoHttp": {
		"org.osgi.service.http.port": "8080",
		"org.osgi.service.http.host": "0.0.0.0",
		"org.apache.felix.http.context_path": "civitas/",
		"org.apache.felix.http.name": "Civitas Http Whiteboard",
		"org.apache.felix.http.runtime.init.id": "civitas",
		"org.apache.felix.http.runtime.init.rest": "true",
		"org.apache.felix.http.debug" : true
	},
	"JakartarsServletWhiteboardRuntimeComponent~civitasrest": {
		"jersey.jakartars.whiteboard.name": "civitasrest",
		"jersey.context.path": "rest",
		"osgi.http.whiteboard.target": "(rest=true)"
	}
}
