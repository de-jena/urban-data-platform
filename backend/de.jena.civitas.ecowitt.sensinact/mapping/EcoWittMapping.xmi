<?xml version="1.0" encoding="UTF-8"?>
<mapping:ProviderMapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:mapping="https://eclipse.org/sensinact/core/mapping/1.0"
    mid="ecowitt-s">
  <timestamp>
    <featurePath
        xsi:type="ecore:EAttribute"
        href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/dateUTC"/>
  </timestamp>
  <name>
    <featurePath
        xsi:type="ecore:EAttribute"
        href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/passkey"/>
  </name>
  <providerClasses
      href="http://www.ecowitt.com/weatherstation#//EcoWittWeather"/>
  <services
      mid="weather">
    <name name="weather"/>
    <resources
        name="Luftfeuchtigkeit"
        unit="%"
        timestamp="//@timestamp"
        mid="huminity">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/huminityIndoor"/>
    </resources>
    <resources
        name="Temperatur"
        unit="F"
        timestamp="//@timestamp"
        mid="temperature">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/temperaturOutdoor"/>
    </resources>
    <resources
        name="barometrischerDruck"
        unit="Pa"
        timestamp="//@timestamp"
        mid="baromabsin">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/barometerAbs"/>
    </resources>
    <resources
        name="Windrichtung"
        unit="Degree"
        timestamp="//@timestamp"
        mid="winddir">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/windDirection"/>
    </resources>
    <resources
        name="Windgeschwindigkeit"
        unit="km/h"
        timestamp="//@timestamp"
        mid="windspeed">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/windSpeedMph"/>
    </resources>
    <resources
        name="Sonnenstrahlung"
        unit="W/mÂ²"
        timestamp="//@timestamp"
        mid="radiation">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/solarRadiation"/>
    </resources>
    <resources
        name="UVIndex"
        unit=""
        timestamp="//@timestamp"
        mid="uv">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/uvIndex"/>
    </resources>
    <resources
        name="Niederschlag"
        unit="mm/h"
        timestamp="//@timestamp"
        mid="rainratein">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/rainRate"/>
    </resources>
    <resources
        name="BatteryStatus"
        unit=""
        timestamp="//@timestamp"
        mid="battery">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/wh65BatteryStatus"/>
