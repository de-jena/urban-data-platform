    <resources
        name="RelativerLuftdruck"
        unit="Pa"
        timestamp="//@timestamp"
        mid="baromrelin">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/barometerRel"/>
    </resources>
    <resources
        name="Dampfdruckdefizit"
        unit="kPa"
        timestamp="//@timestamp"
        mid="vpd">
      <eType
          xsi:type="ecore:EDataType"
          href="http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
      <valueFeature
          xsi:type="ecore:EAttribute"
          href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/vpd"/>
    </resources>
  </services>
  <admin mid="admin"
      latitude="8.531689"
      longitude="51.952503">
    <name name="Admin"/>
    <friendlyNameFeature
        xsi:type="ecore:EAttribute"
        href="http://www.ecowitt.com/weatherstation#//EcoWittWeather/stationType"/>
    <providerPackage
        href="http://www.ecowitt.com/weatherstation#/"/>
  </admin>
</mapping:ProviderMapping>
