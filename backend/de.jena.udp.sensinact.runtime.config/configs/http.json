{
    ":configurator:resource-version": 1,
    ":configurator:version": "1.0.0",
    ":configurator:symbolic-name": "UDP Http",
    "org.apache.felix.http~mdoHttp": {
        "org.osgi.service.http.port": "8080",
        "org.osgi.service.http.host": "0.0.0.0",
        "org.apache.felix.http.context_path": "udp/",
        "org.apache.felix.http.name": "UDP Http Whiteboard",
        "org.apache.felix.http.runtime.init.id": "udp",
        "org.apache.felix.http.runtime.init.rest": "true",
        "org.apache.felix.http.debug": true
    },
    "JakartarsServletWhiteboardRuntimeComponent~udprest": {
        "jersey.jakartars.whiteboard.name": "udprest",
        "jersey.context.path": "rest",
        "osgi.http.whiteboard.target": "(rest=true)"
    },
    "de.jena.udp.rest.cors.CorsConfig": {
        "enabled": true,
        "allowedOrigins": ["$[env:CORS_ALLOWED_ORIGINS;default=*]"],
        "allowedMethods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"],
        "allowedHeaders": ["Origin", "Content-Type", "Accept", "Authorization", "X-Requested-With"],
        "exposedHeaders": ["Content-Length", "Content-Type"],
        "allowCredentials": false,
        "maxAge": 3600
    }
}
